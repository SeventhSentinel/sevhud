	void DrawStatusBarPowerups(void)
	{
		// Draw the background first
		DrawImage("graphics/SevHUD/chex/st_extra.png", (-53, 168), DI_ITEM_OFFSETS);

		let lightamp = Powerup(CPlayer.mo.FindInventory("PowerLightAmp"));
