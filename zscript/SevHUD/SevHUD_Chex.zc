			DrawBar("graphics/SevHUD/chex/st_lightamp.png", "graphics/SevHUD/chex/st_lightampback.png", int(Ceil(double(lightamp.EffectTics))), int(Ceil(double(lightamp.MaxEffectTics))), (-21, 172), 0, SHADER_VERT|SHADER_REVERSE, DI_ITEM_OFFSETS, 1.0);

		let radsuit = Powerup(CPlayer.mo.FindInventory("PowerIronFeet"));
		if (radsuit)
			DrawBar("graphics/SevHUD/chex/st_radsuit.png", "graphics/SevHUD/chex/st_radsuitback.png", int(Ceil(double(radSuit.EffectTics))), int(Ceil(double(radSuit.MaxEffectTics))), (-48, 172), 0, SHADER_VERT|SHADER_REVERSE, DI_ITEM_OFFSETS, 1.0);

